{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "typeRoots": [
        "node_modules/@types"
      ]
    },
    "include": ["src/**/*.ts"]
  }
  
